		</classes>
	</test>
	<test name="AccessManageContact">
		<classes>
			<class name="com.sevenrmartsupermarket.tests.DashBoardTest"></class>
			<class name="com.sevenrmartsupermarket.tests.LoginTests"></class>
		</classes>
	</test>
</suite>